public class Sample1 {
    public static void main(String[] args) {
        int num = 29;
        int a = num%10;
        int b = num/10;
        int res = (a+b) + (a*b);
        if(res == num)
          System.out.println(num + " is special 2 digit number");
        else
          System.out.println(num + " is not a special 2 digit number");
    }
}
